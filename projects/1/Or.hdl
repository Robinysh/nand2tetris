/**
 * Or gate:
 * if (a or b) out = 1, else out = 0 
 */
CHIP Or {
    IN a, b;
    OUT out;

    PARTS:
    Not(in=a, out=NotA);
    Not(in=b, out=NotB);
    And(a=NotA, b=NotB, out=NotAOrNotB);
    Not(in=NotAOrNotB, out=out);
    //// Replace this comment with your code.
}
